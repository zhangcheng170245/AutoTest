[
  {
    "description":"模拟一个没有参数的get请求",
    "request":{
      "uri":"/getdemo",
      "method":"get"
    },
    "response":{
      "text":"这是一个没有参数的get请求",
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      }
    }
  },
  {
    "description":"模拟一个有参数的get请求",
    "request":{

      "uri":"/getwithparam",
      "method":"get",
      "queries":{
        "name":"shadiao",
        "age":"18"
      }
    },

    "response":{
      "text":"劳资回来了",
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      }
    }
  }
]